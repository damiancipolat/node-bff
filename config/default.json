{
  "server": {
    "port": "8080"
  },
  "proxy": {
    "map": [
      {
        "target": "https://5f3a062e2300b100169a84f7.mockapi.io",
        "endpoint": "/movies",
        "routes": [
          {
            "method": "get",
            "path": "/movie",
